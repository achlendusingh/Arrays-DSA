#include <iostream> 
#include <unordered_map> 
using namespace std; 
  
// Driver code 
int main() 
{
   int a[]={1,5,10,20,40,80};
   int b[]={6,7,20,80,100};
   int c[]={3,4,15,20,30,70,80,120};
    unordered_map<int,int>hash;
    for(int i=0;i<6;i++)
    {
        if(hash[a[i]]==0)
        hash[a[i]]++;
    }
    for(int i=0;i<5;i++)
    {
        if(hash[b[i]]==1)
        hash[b[i]]++;
    }
    for(int i=0;i<8;i++)
    {
        if(hash[c[i]]==2)
        hash[c[i]]++;
    }
    for (int i=0;i<6;i++)
    {
        if (hash[a[i]]==3)
        {
            cout<<a[i]<<" is the common element"<<endl;
        }
    }
    return 0;
}
